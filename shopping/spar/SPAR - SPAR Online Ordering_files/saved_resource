// place any jQuery/helper plugins in here, instead of separate, slower script files.

var mobileMenu = function() {
     if ($(window).width() <= 576) {
     $('.mobile-menu').addClass('collapse');
  }
     else {
        $('.mobile-menu').removeClass('collapse');
     }
};;



